ALLOWED_MAKE_VERSIONS :=3.81 3.82

MAKE_VERSION_IS_SUPPORTED := $(strip $(foreach v,$(ALLOWED_MAKE_VERSIONS), $(findstring $v,$(MAKE_VERSION))))
